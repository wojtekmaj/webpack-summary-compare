{
  "name": "webpack-summary-compare",
  "version": "1.0.0",
  "description": "Generates a summary of differences between two Webpack summaries",
  "scripts": {
    "build": "parcel build src/index.html --public-url ./ --cache-dir ./.cache/prod --no-source-maps",
    "clean": "rimraf dist",
    "lint": "eslint src/ --ext .jsx,.js",
    "start": "parcel src/index.html --cache-dir ./.cache/dev",
    "test": "yarn lint"
  },
  "author": {
    "name": "Wojciech Maj",
    "email": "kontakt@wojtekmaj.pl"
  },
  "license": "MIT",
  "dependencies": {
    "merge-class-names": "^1.1.1",
    "prop-types": "^15.6.2",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-markdown": "^4.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "~7.2.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jsx-a11y": "^6.3.0",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.0",
    "less": "^3.8.1",
    "parcel": "^2.0.0-beta.1",
    "rimraf": "^2.6.1"
  }
}
