{
  "name": "webpack-summary-compare",
  "version": "1.0.0",
  "description": "Generates a summary of differences between two Webpack summaries",
  "main": "src/index.html",
  "scripts": {
    "build": "yarn clean && parcel build src/index.html --public-url ./ --cache-dir ./.cache/prod",
    "clean": "rimraf dist",
    "test": "yarn run test-eslint",
    "test-eslint": "eslint src/ --ext .jsx,.js",
    "start": "parcel src/index.html --cache-dir ./.cache/dev"
  },
  "author": {
    "name": "Wojciech Maj",
    "email": "kontakt@wojtekmaj.pl"
  },
  "license": "MIT",
  "dependencies": {
    "merge-class-names": "^1.1.1",
    "prop-types": "^15.6.2",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-markdown": "^4.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.6.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "less": "^3.8.1",
    "parcel-bundler": "^1.12.2",
    "rimraf": "^2.6.1"
  }
}
